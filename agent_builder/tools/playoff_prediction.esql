FROM nba_team_season_stats
| WHERE season == "2025-26"
| EVAL total_games = wins + losses
| EVAL projected_wins = CASE(
    total_games > 0, ROUND(82 * win_pct),
    0
  )
| EVAL playoff_probability = CASE(
    win_pct >= 0.750, 1.00,
    win_pct >= 0.700, 0.80,
    win_pct >= 0.65, 0.75,
    win_pct >= 0.600, 0.70,
    win_pct >= 0.575, 0.60,
    win_pct >= 0.550, 0.50,
    win_pct >= 0.500, 0.30,
    0.05
  )
| EVAL seed_projection = CASE(
    playoff_probability >= 1, 1,
    playoff_probability >= 0.80, 2,
    playoff_probability >= 0.80, 2,
    playoff_probability >= 0.80, 2,


    playoff_probability >= 0.60, 3,
    playoff_probability >= 0.40, 5,


  )
| KEEP team_name, wins, losses, total_games, projected_wins, win_pct, seed_projection, playoff_probability
| SORT projected_wins DESC
